LABEL SUBSTR_WHILE
JUMPIFEQ END_WHILE_SUBSTR LF@START LF@END

GETCHAR LF@CHAR_VAR LF@%1 LF@START
CONCAT LF@STR LF@STR LF@CHAR_VAR
ADD LF@START LF@START int@1

JUMP SUBSTR_WHILE
LABEL END_WHILE_SUBSTR

MOVE LF@%1r LF@STR
POPFRAME
RETURN

LABEL RETURN_BLANK_SUBSTR
